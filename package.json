{
  "name": "risk-calculation-data",
  "version": "1.0.0",
  "description": "Financial risk calculation platform with real-time data from FMP and Quandl APIs",
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "analyze": "node src/scripts/run_correlation_analysis.js",
    "visualize": "python3 src/visualization/CorrelationVisualizer.py",
    "install-python-deps": "pip3 install -r requirements.txt"
  },
  "keywords": ["finance", "risk", "calculation", "api", "fmp", "quandl", "correlation", "statistics"],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "node-cron": "^3.0.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.0"
  }
}